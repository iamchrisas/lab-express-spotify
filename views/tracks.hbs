<h1>Tracks</h1>
<ul>
  {{#each tracks}}
    <li>
      <p>{{this.name}}</p>
      {{#if this.preview_url}}
        <audio controls>
          <source src="{{this.preview_url}}" type="audio/mpeg">
          Your browser does not support the audio element.
        </audio>
      {{else}}
        No preview available.
      {{/if}}
    </li>
  {{/each}}
</ul>
